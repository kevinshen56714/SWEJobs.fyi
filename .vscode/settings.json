{
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "eslint.enable": true,
  "eslint.validate": [
    "typescript",
    "typescriptreact"
  ],
  "sort-imports.on-save": true
}
